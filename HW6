const tinhDienTich = (chieuDai, chieuRong) => chieuDai * chieuRong;
const tinhChuVi = (chieuDai, chieuRong) => 2 * (chieuDai + chieuRong);




const chieuDai = 10;
const chieuRong = 2;



console.log(`Diện tích: ${tinhDienTich(chieuDai, chieuRong)}`);
console.log(`Chu vi: ${tinhChuVi(chieuDai, chieuRong)}`);

